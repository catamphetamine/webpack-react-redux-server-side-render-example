{
  "name": "webpack-react-redux-isomorphic-render-example",
  "version": "1.0.0",
  "description": "an alternative to using webpack-isomorphic-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/halt-hammerzeit/webpack-react-redux-isomorphic-render-example.git"
  },
  "homepage": "https://github.com/halt-hammerzeit/webpack-react-redux-isomorphic-render-example",
  "dependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "bluebird": "^2.10.0",
    "bunyan": "^1.3.5",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.19.0",
    "deep-equal": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "fs-extra": "^0.24.0",
    "http-proxy": "^1.11.1",
    "json-loader": "^0.5.2",
    "minimist": "^1.1.1",
    "moment": "^2.10.6",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.2.13",
    "postcss-loader": "^0.9.1",
    "pretty-error": "^1.1.2",
    "print-error": "^0.1.8",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-isomorphic-render": "^6.0.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.4.1",
    "react-styling": "^1.3.0",
    "redux": "^3.0.0",
    "sass-loader": "^3.1.2",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.12.4",
    "universal-webpack": "^0.1.31",
    "url-loader": "^0.5.5",
    "web-service": "^0.2.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-plugin-react-intl": "^2.1.1",
    "babel-plugin-react-transform": "^2.0.0",
    "express": "^4.13.3",
    "nodemon": "^1.8.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "1.2.6",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.1"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all development-prepare-server-build development",
    "development": "npm-run-all --parallel development-build-client development-build-server development-server",
    "development-prepare-server-build": "universal-webpack --settings ./webpack/universal-webpack-settings.js prepare",
    "development-build-client": "node ./webpack/webpack-dev-server.entry.js",
    "development-build-server": "webpack --config ./webpack/webpack.config.server.development.entry.js --watch --colors --display-error-details",
    "development-api-server": "nodemon ./code/api-server/entry.js --watch ./code/api-server --watch ./code/common",
    "development-page-server": "nodemon ./code/page-server/entry.js --watch ./code/page-server --watch ./code/common --watch ./build/server",
    "development-page-server-delayed": "npm-run-all delay development-page-server",
    "development-web-server": "nodemon ./code/web-server/entry.js --watch ./code/web-server --watch ./code/common",
    "development-web-server-delayed": "npm-run-all delay delay development-web-server",
    "development-server": "npm-run-all --parallel development-api-server development-page-server-delayed development-web-server-delayed",
    "delay": "node ./code/sleep 500",
    "production": "npm-run-all production-build-client production-build-server production-server",
    "production-build-client": "webpack --colors --display-error-details --config ./webpack/webpack.config.client.production.entry.js",
    "production-build-server": "webpack --colors --display-error-details --config ./webpack/webpack.config.server.production.entry.js",
    "production-api-server": "node ./code/api-server/entry.js --production",
    "production-page-server": "node ./code/page-server/entry.js",
    "production-web-server": "node ./code/web-server/entry.js --production",
    "production-server": "npm-run-all --parallel production-api-server production-page-server production-web-server"
  },
  "author": "Halt Hammerzeit",
  "license": "MIT",
  "engines": {
    "node": "4.1.1"
  }
}
