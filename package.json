{
  "private": true,
  "name": "webpack-react-redux-server-side-render-example",
  "version": "1.0.0",
  "type": "module",
  "description": "A sample React/Redux/Webpack project with Server-Side Rendering",
  "repository": {
    "type": "git",
    "url": "https://github.com/catamphetamine/webpack-react-redux-server-side-render-example.git"
  },
  "homepage": "https://github.com/catamphetamine/webpack-react-redux-server-side-render-example",
  "scripts": {
    "start": "npm run production",
    "dev": "npm-run-all development:client:rendering-service:build:prepare build:commonjs:package.json development",
    "development": "npm-run-all --parallel development:client:build development:client:rendering-service:build development:services",
    "development:client:rendering-service:build:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development:client:build": "node --experimental-json-modules node_modules/webpack/bin/webpack  serve --hot --config webpack/webpack.config.client.development.js",
    "development:client:rendering-service:build": "node --experimental-json-modules node_modules/webpack/bin/webpack  --mode development --config ./webpack/webpack.config.server.development.js --watch --color --stats-errors --stats-error-details --module-strict-export-presence",
    "development:api": "nodemon ./api/index.js --watch ./api --no-update-notifier",
    "development:client:rendering-service": "nodemon ./rendering-service/index.js --watch ./rendering-service --watch ./build/server --no-update-notifier",
    "development:services": "npm-run-all --parallel development:api development:client:rendering-service",
    "production": "npm-run-all production:client:build production:client:rendering-service:build build:commonjs:package.json production:services",
    "production:client:build": "node --experimental-json-modules node_modules/webpack/bin/webpack  --mode production --color --stats-errors --stats-error-details --module-strict-export-presence --config ./webpack/webpack.config.client.production.js",
    "production:client:rendering-service:build": "node --experimental-json-modules node_modules/webpack/bin/webpack  --mode production --color --stats-errors --stats-error-details --module-strict-export-presence --config ./webpack/webpack.config.server.production.js",
    "production:api": "cross-env NODE_ENV=production node ./api/index.js",
    "production:client:rendering-service": "cross-env NODE_ENV=production node ./rendering-service/index.js",
    "production:proxy": "cross-env NODE_ENV=production node ./proxy-server/index.js",
    "production:services": "npm-run-all --parallel production:api production:client:rendering-service production:proxy",
    "build:commonjs:package.json": "node ./runnable/create-commonjs-package-json.js"
  },
  "author": "catamphetamine <purecatamphetamine@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.3.2",
    "core-js": "^3.27.1",
    "easy-react-form": "^1.3.4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lokijs": "^1.5.12",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pages": "^0.6.26",
    "react-redux": "^8.0.5",
    "react-responsive-ui": "^0.15.34",
    "react-time-ago": "^7.2.1",
    "redux": "^4.2.0",
    "regenerator-runtime": "^0.13.11",
    "serverless-functions": "^0.5.3",
    "uuid": "^9.0.0",
    "web-service": "^0.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@pmmmwh/react-refresh-webpack-plugin": "github:pmmmwh/react-refresh-webpack-plugin",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-calc": "^8.2.4",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^5.3.6",
    "universal-webpack": "^0.8.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
