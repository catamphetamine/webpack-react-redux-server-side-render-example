{
  "presets":
  [
    "@babel/preset-react",
    ["@babel/preset-env", { modules: false }]
  ],

  "plugins":
  [
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose" : true }]
  ]
}